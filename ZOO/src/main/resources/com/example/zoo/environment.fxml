<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f7de5f;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.zoo.EnvironmentController">
    <children>
        <TableView fx:id="environmentTable" layoutX="28.0" layoutY="69.0" prefHeight="278.0" prefWidth="544.0">
            <columns>
                <TableColumn fx:id="eidColumn" prefWidth="75.0" text="EID" />
                <TableColumn fx:id="nameColumn" prefWidth="150.0" text="Name" />
                <TableColumn fx:id="typeColumn" prefWidth="150.0" text="Type" />
                <TableColumn fx:id="surfaceColumn" prefWidth="150.0" text="Surface" />
            </columns>
        </TableView>
        <ComboBox fx:id="environmentComboBox" layoutX="28.0" layoutY="29.0" prefWidth="150.0" />
        <Button layoutX="207.0" layoutY="29.0" mnemonicParsing="false" onAction="#onAddEnvironmentClick" text="Add Environment" />
        <Button layoutX="333.0" layoutY="29.0" mnemonicParsing="false" onAction="#onDeleteEnvironmentClick" text="Delete Environment" />
      <Button fx:id="buttonb" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#onButtonbClick" text="Back" />
      <Label fx:id="l1" layoutX="133.0" layoutY="365.0" />
    </children>

</AnchorPane>
